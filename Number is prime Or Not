#include <stdio.h>

int main() {
    int i, x;
    printf("enter x:");
    scanf("%d", &x);
    printf("%d\n",x);
    int isPrime = 1;
    for(int i=2; i<x; i+=1){
    printf("%d %d \n",i, x%i);
        if((x%i)==0){
            isPrime = 0;
            break;
        }
    }
    if(isPrime==1)
            printf("%d is prime number.\n",x);
        else
            printf("%d is not a prime number.\n",x);
            return 0;
    
}
