#include <stdio.h>

int main() {
    int i, x;
    printf("enter x:");
    scanf("%d", &x);
    printf("%d\n",x);
    for(int i=2; i<x; i+=1){
        if((x%i)==0)
            printf("%d is not a prime number.\n",x);
        else
            printf("%d is a prime number.\n",x);
            return 0;
    }
}
